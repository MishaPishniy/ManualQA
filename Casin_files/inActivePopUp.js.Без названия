(()=>{var t=window.$;t(".inActiveModal").each(function(){let c=!0,s=!1,o=t(this),i=t(o.data("trigger-el")),l=o.data("delay")||5e3,a=o.find("[data-close]");t("body").append(o),t(window).on("scroll",()=>{let n=i.offset()?i.offset().top+i.outerHeight():t(window).outerHeight();s=t(window).scrollTop()>n});function d(){c&&s&&o.fadeIn()}function u(){o.fadeOut(),c=!1}(function(){let n;t(window).on("scroll",e),t(window).on("touchstart",e),t(document).on("keypress",e),t(document).on("mousemove",e),t(document).on("click",e);function e(){clearTimeout(n),n=setTimeout(d,l)}})(),a.on("click",u)});})();
